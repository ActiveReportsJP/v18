  <Script><![CDATA[public void ActiveReport_ReportStart()
{
	// 固定部分の色を、一括でオレンジ色に設定します。
	System.Drawing.Color clr = System.Drawing.Color.Orange;
	// 固定部分が台紙に既に印刷されている場合は、
	// Color.Transparentに設定することで、
	// 透明（出力させない）にすることも可能です。
	int i;

	// Lineのカラーを設定します。
	for (i = 1; i <= 44; i++)
	{
		((GrapeCity.ActiveReports.SectionReportModel.Line) (rpt.Sections["Detail"].Controls["Line" + i.ToString()])).LineColor = clr;
	}
	// Labelのカラーを設定します。
	for (i = 1; i <= 35; i++)
	{
		((GrapeCity.ActiveReports.SectionReportModel.Label) (rpt.Sections["Detail"].Controls["Label" + i.ToString()])).ForeColor = clr;
	}
	// Shapeのカラーを設定します。
	this.Shape1.LineColor = clr;
}


public void Detail_Format()
{
	int i;
	string strKingaku = "******" + txtKingaku1_1.Text;
	string strData;
	// TextBoxに金額を一桁ずつセットします。
	// 数字のない桁は、"*"で埋めます。
	for (i = 1; i <= 6; i++)
	{
		strData = strKingaku.Substring(strKingaku.Length - i, 1);
		((GrapeCity.ActiveReports.SectionReportModel.TextBox) (rpt.Sections["Detail"].Controls["txtKingaku1_" + i.ToString()])).Text = strData;
		((GrapeCity.ActiveReports.SectionReportModel.TextBox) (rpt.Sections["Detail"].Controls["txtKingaku2_" + i.ToString()])).Text = strData;
	}
	// バーコードのキャプション作成し、
	// TextBoxにセットし直します。
	// AI（データ項目用）
	strData = "(" + txtBarcode.Text.Substring(0, 2) + ")";
	// メーカーコード
	strData += txtBarcode.Text.Substring(2, 6) + "-";
	// 自由使用欄
	strData += txtBarcode.Text.Substring(8, 22) + "\r\n";
	// 支払期限年月日
	strData += txtBarcode.Text.Substring(30, 6) + "-";
	// 収入印紙の要不要
	strData += txtBarcode.Text.Substring(36, 1) + "-";
	// 請求金額
	strData += txtBarcode.Text.Substring(37, 6) + "-";
	// チェックディジット
	strData += txtBarcode.Text.Substring(43, 1);
	txtBarcode.Text = strData;

	//
	// バーコードの設定を行います。
	// 

	//*--- 注意 ---------------------------------------------*
	// 「UCC／EAN－128による標準料金代理収納ガイドライン」に基づくバーコード
	// を出力する為には、300dpi 以上の出力解像度を持ったプリンタを使用する
	// 必要があります。読み取り精度を考慮すると 600dpi 以上が望ましいといえます。
	//また、インクのにじみや、反射率等も読み取り精度に大きく影響します。
	// バーコードが上手く読み取れない場合は、黒バーのドット補正をお試しください。
	//*------------------------------------------------------*

	// 黒バーのドット補正を行います。
	// 実際に印刷を行うプリンタの解像度と最小モジュールサイズを指定し、
	// 最小モジュール幅をドット単位で補正します。

	// 解像度と最小モジュールサイズの推奨値は次のとおりです。
	//---------------------------------
	// 解像度 | 最小モジュール幅
	// 1200   | 8
	// 720    | 4 または 5
	// 600    | 4
	// 480    | 3
	// 400    | 3
	// 360    | 2
	// 300    | 2
	//---------------------------------

	// サイズの自動調整を不可に設定します。
	this.Barcode1.AutoSize = false;

	// プリンタの解像度(dpi)を指定します。
	this.Barcode1.Code128.Dpi = 600;

	// 最小モジュール幅を指定します。600dpiの場合は4ドットを指定します。
	this.Barcode1.Code128.ModuleSize = 4;

	// 黒バーを-1ドット補正します。
	this.Barcode1.Code128.BarAdjust = -1;
}


