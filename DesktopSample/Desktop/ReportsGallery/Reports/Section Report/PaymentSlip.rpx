{
	
}


public void Detail_AfterPrint()
{
	int i;
	// 名前の後ろに"　様"を追加します。
	for (i = 1; i <= 2; i++)
	{
		((GrapeCity.ActiveReports.SectionReportModel.TextBox) (rpt.Sections["Detail"].Controls["txtName" + i.ToString()])).Text += "　様";
	}
}


